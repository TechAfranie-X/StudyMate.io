{
  "name": "studymate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "lint": "cd client && npm run lint",
    "preview": "cd client && npm run preview",
    "server": "cd server && npm start",
    "server:dev": "cd server && npm run dev",
    "server:simple": "cd server && npm run server:simple",
    "mock-server": "cd server && npm run mock-server",
    "test-server": "cd server && npm run test-server",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "cd server && npm run db:generate",
    "db:migrate": "cd server && npm run db:migrate",
    "db:reset": "cd server && npm run db:reset",
    "db:seed": "cd server && npm run db:seed",
    "db:studio": "cd server && npm run db:studio",
    "install:all": "npm install && cd client && npm install && cd ../server && npm install"
  },
  "dependencies": {
    "@prisma/client": "4.16.2",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "framer-motion": "^10.16.4",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/supertest": "^6.0.2",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^30.1.1",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.31",
    "prisma": "^4.16.2",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.3.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "vite": "^4.5.0"
